    "encoded = Dense(512, activation='relu')(input_ratings)\n",
    "encoded = Dropout(0.3)(encoded)  # Dropout layer with a 30% rate\n",
    "encoded = Dense(256, activation='relu')(encoded)\n",
    "encoded = Dense(encoding_dim, activation='relu')(encoded)\n",
